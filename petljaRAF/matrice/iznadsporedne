#include <stdio.h>

#define N 100
#define M 100

int main() {
    int matrix[N][M];
    int n, m, i, j, zbir;

    printf("Enter the number of rows (n): ");
    scanf("%d", &n);

    printf("Enter the number of columns (m): ");
    scanf("%d", &m);

    printf("Enter elements of the matrix:\n");
    for(i = 0; i < n; i++) {
        for(j = 0; j < m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
        for(i = 0; i < n; i++) {
        for(j = 0; j < n-i-1; j++) {
                zbir+=matrix[i][j];


        }
        }
printf("%d",zbir);
    return 0;
}
