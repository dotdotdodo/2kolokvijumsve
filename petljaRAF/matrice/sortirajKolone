#include <stdio.h>

void sortiraj_kolone(int matrica[100][100], int N, int M) {
    int i, j, k, temp;
    for (i = 0; i < M; i++) {
        for (j = 0; j < N-1; j++) {
            for (k = 0; k < N-j-1; k++) {
                if (matrica[k][i] > matrica[k+1][i]) {
                    temp = matrica[k][i];
                    matrica[k][i] = matrica[k+1][i];
                    matrica[k+1][i] = temp;
                }
            }
        }
    }
}

int main() {
    int N, M, i, j;
    int matrica[100][100];
    scanf("%d %d", &N, &M);
    for (i = 0; i < N; i++) {
        for (j = 0; j < M; j++) {
            scanf("%d", &matrica[i][j]);
        }
    }
    sortiraj_kolone(matrica, N, M);
    for (i = 0; i < N; i++) {
        for (j = 0; j < M; j++) {
            printf("%d ", matrica[i][j]);
        }
        printf("\n");
    }
    return 0;
}
