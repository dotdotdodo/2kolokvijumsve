#include <stdio.h>

void ucitaj(int n, int m, int mat[][100]) {
    int i, j;
    for (i = 0; i < n; i++)
        for (j = 0; j < m; j++)
            scanf("%d", &mat[i][j]);
}

void ispisi(int n, int m, int mat[][100]) {
    int i, j;
    for (i = 0; i < n; i++, putchar('\n'))
        for (j = 0; j < m; j++)
            printf("%d ", mat[i][j]);
}
void generisi_mat(int n, int m,int mat[][m])
{
    for(int i=9;i<n;i++){
        for(int j=0;j<m;j++){
                mat[i][j]=rand()%10;
        }
    }
}
